{
  "name": "portfolio",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:fr": "ng serve --configuration=fr",
    "build": "ng build",
    "build:ci": "ng build --prod",
    "build:fr": "ng build --configuration=production-fr",
    "build:all": "ng build --prod & ng build --configuration=production-fr",
    "test": "ng test",
    "test:ci": "ng test --watch=false --code-coverage=true",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "serve:ci": "node server.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "lighthouse:home": "node_modules/lighthouse-ci/bin/cli --performance=90 --accessibility=90 --best-practice=90 --report=coverage --emulated-form-factor desktop http://localhost:4000",
    "lighthouse:ci": "start-server-and-test serve:ci http://localhost:4000 lighthouse:home",
    "cypress": "cypress",
    "e2e:run": "cypress run",
    "e2e:ci": "start-server-and-test serve:ci http://localhost:4000 e2e:run",
    "translate:extract": "ng xi18n --output-path i18n"
  },
  "private": true,
